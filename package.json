{
	"name": "sf-down",
	"version": "0.1.0",
	"description": "A simple and efficient download tool for Sourceforge.",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"bin": "dist/cjs/cli.js",
	"scripts": {
		"build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
		"prepublishOnly": "npm run build && shx cp package.json dist/",
		"lint:fix": "biome lint --write",
		"dev": "tsx src/cli.ts",
		"test": "tsx test/basic.test.ts"
	},
	"engines": {
		"node": ">=18"
	},
	"repository": {
		"type": "https",
		"url": "https://github.com/whyun-pages/sf-down.git"
	},
	"keywords": [
		"sourceforge",
		"download",
		"tool"
	],
	"author": "yunnysunny",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.2.3",
		"@types/node": "^24.3.1",
		"shx": "^0.4.0",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@xmldom/xmldom": "^0.8.11",
		"commander": "^14.0.0"
	}
}
